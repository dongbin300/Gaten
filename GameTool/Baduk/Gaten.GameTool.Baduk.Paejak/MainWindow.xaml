<Window x:Class="Gaten.GameTool.Baduk.Paejak.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gaten.GameTool.Baduk.Paejak"
        mc:Ignorable="d"
        Title="바둑 패작 프로그램 for 한큐바둑" Height="200" Width="300">
    <Grid>
        <StackPanel>
            <Button x:Name="TestButton" Content="Test" Height="40" Click="TestButton_Click"/>
            <Button x:Name="StartButton" Content="Start [Num1]" Height="40" Click="StartButton_Click"/>
            <Button x:Name="StopButton" Content="Stop [Num2]" Height="40" Click="StopButton_Click"/>
        </StackPanel>
    </Grid>
</Window>

<Window x:Class="Gaten.Windows.MintPanda.SystemMonitorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gaten.Windows.MintPanda"
        mc:Ignorable="d"
        Deactivated="Window_Deactivated"
        WindowStyle="None" AllowsTransparency="True" Topmost="True" Background="Transparent"
        ShowInTaskbar="False"
        Title="SystemMonitorWindow" Height="300" Width="200">
    <StackPanel>
        <TextBlock x:Name="SystemInfoText" FontSize="11" TextWrapping="Wrap"/>
    </StackPanel>
</Window>

<Window x:Class="Gaten.Windows.MintPanda.Contents.RandomWord"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gaten.Windows.MintPanda.Contents"
        xmlns:wpf="clr-namespace:Gaten.Net.Wpf.Controls;assembly=Gaten.Net.Wpf"
        mc:Ignorable="d"
        Title="RandomWord" Height="80" Width="500"
        Style="{StaticResource ContentWindow}" WindowStartupLocation="CenterScreen" MouseDown="Window_MouseDown">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="RandomWordText" TextWrapping="Wrap" />
            <Button x:Name="RandomWordButton3" Content="생성" Click="RandomWordButton_Click" />
        </StackPanel>
    </Grid>
</Window>

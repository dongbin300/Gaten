<Window x:Class="Gaten.Windows.MintPanda.Contents.Dictionary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gaten.Windows.MintPanda.Contents"
        mc:Ignorable="d"
        Title="Dictionary" Height="333" Width="400"
        Style="{StaticResource ContentWindow}" WindowStartupLocation="CenterScreen" MouseDown="Window_MouseDown">
    <Grid>
        <StackPanel>
            <TextBox x:Name="DictTextBox" KeyDown="DictTextBox_KeyDown" Width="Auto" />
            <ListBox x:Name="DictListBox" Height="300" ScrollViewer.HorizontalScrollBarVisibility="Auto"/>
        </StackPanel>
    </Grid>
</Window>

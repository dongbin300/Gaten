<Window x:Class="Gaten.Windows.MintPanda.Contents.RNG"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gaten.Windows.MintPanda.Contents"
        mc:Ignorable="d"
        Title="RNG" Height="40" Width="300"
        Style="{StaticResource ContentWindow}" WindowStartupLocation="CenterScreen" MouseDown="Window_MouseDown">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBox x:Name="RNGMin" Width="50" Text="1" />
            <TextBlock Text="~" />
            <TextBox x:Name="RNGMax" Width="50" Text="100" />
            <Button x:Name="RNGButton" Content="생성" Click="RNGButton_Click" />
            <TextBlock x:Name="RNGResultText" />
        </StackPanel>
    </Grid>
</Window>

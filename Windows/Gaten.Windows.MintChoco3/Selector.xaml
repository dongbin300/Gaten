<Window x:Class="Gaten.Windows.MintChoco3.Selector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gaten.Windows.MintChoco3"
        mc:Ignorable="d"
       WindowStyle="None" ResizeMode="NoResize"
        Title="Selector" Height="600" Width="250"
        Loaded="Window_Loaded" Unloaded="Window_Unloaded">

    <Grid Background="{StaticResource SoftBlack}">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <!--<TextBlock x:Name="KeyText" Grid.Row="0" FontSize="32"
                   Text="{Binding KeyText}"
                   Style="{StaticResource DarkTextBlock}"/>-->
        <ListBox x:Name="ModuleList"
                 Style="{StaticResource DarkListBox}">
        </ListBox>
    </Grid>
</Window>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyName>SuperMonitor</AssemblyName>
    <ApplicationIcon>super.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="super.ico" />
    <None Remove="super.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="super.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Binance.Net" Version="8.6.2" />
    <PackageReference Include="XCommas.Net" Version="1.0.38" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Net\Gaten.Net.Wpf\Gaten.Net.Wpf.csproj" />
    <ProjectReference Include="..\..\Net\Gaten.Net\Gaten.Net.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="super.ico" />
    <Resource Include="super.png" />
  </ItemGroup>

</Project>

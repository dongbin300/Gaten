<Window x:Class="Gaten.Windows.RestoreWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gaten.Windows.RestoreWindow"
        mc:Ignorable="d"
        Title="Restore Window" Height="80" Width="200" ResizeMode="NoResize">
    <Grid>
        <UniformGrid Rows="1">
            <Button x:Name="SaveButton" Content="저장하기" Click="SaveButton_Click"/>
            <Button x:Name="LoadButton" Content="불러오기" Click="LoadButton_Click"/>
        </UniformGrid>
    </Grid>
</Window>

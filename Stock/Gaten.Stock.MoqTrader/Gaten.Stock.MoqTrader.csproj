    <PackageReference Include="Skender.Stock.Indicators" Version="2.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Net\Gaten.Net.Wpf\Gaten.Net.Wpf.csproj" />
    <ProjectReference Include="..\..\Net\Gaten.Net\Gaten.Net.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Images\moq.ico" />
